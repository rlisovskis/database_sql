        "%%sql\n",
        "DROP TABLE IF EXISTS Monster2Class;\n",
        "CREATE TABLE Monster2Class(\n",
        "  monster_id INT REFERENCES Monster (monster_id),\n",
        "  class_id INT REFERENCES Class (id),\n",
        "PRIMARY KEY (monster_id, class_id)\n",
        ");"
